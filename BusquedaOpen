import sys
from Busqueda import *

class Busqueda2(QtGui.QDialog):
	def __init__(self, parent=None):
		QtGui.QWidget.__init__(self, parent)
		self.uiBusqueda = Ui_dialog()
		self.uiBusqueda.setupUi(self)
		QtCore.QObject.connect(self.uiBusqueda.pushButtonCancelarBusqueda, QtCore.SIGNAL('clicked()'), self.salirBusqueda)
		#QtCore.QObject.connect(self.uiBusqueda.pushButtonCancelarBusqueda, QtCore.SIGNAL('clicked()'), self.salirBusqueda)
	def salirBusqueda(self):
		exit()

if __name__ == "__main__":
	app = QtGui.QApplication(sys.argv)
	myapp = Busqueda2()
	myapp.show()
	sys.exit(app.exec_())
